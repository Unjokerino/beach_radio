{
  "expo": {
    "name": "Радио Пляж",
    "slug": "beach_radio",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#262626"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "versionCode": 3,
      "permissions": [],
      "package": "com.mironov.beachradio"
    }
  }
}